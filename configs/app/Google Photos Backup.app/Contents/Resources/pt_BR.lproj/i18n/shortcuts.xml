<?xml version="1.0" encoding="utf-8" ?>
<shortcuts>
 <keymap id="0">
  <!-- New label -->
  <item srckey="N" dstkey="" ctrl="1" shift="0" alt="0">
  </item>
  <!-- Add File to Picasa -->
  <item srckey="A" dstkey="O" ctrl="1" shift="0" alt="0">
  </item>
  <!-- Import from -->
  <item srckey="D" dstkey="M" ctrl="1" shift="0" alt="0">
  </item>
