  <item srckey="." dstkey="" ctrl="0" shift="0" alt="0">
  </item>
  <!-- Next Picture -->
  <item srckey="" dstkey="" ctrl="0" shift="0" alt="0">
  </item>
  <!-- Previous Picture -->
  <item srckey="" dstkey="" ctrl="0" shift="0" alt="0">
  </item>
