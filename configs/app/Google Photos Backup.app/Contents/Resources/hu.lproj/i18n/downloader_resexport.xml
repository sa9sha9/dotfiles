<?xml version="1.0" encoding="utf-8" ?>
<Win32Res>
 <RT_DIALOG>
  <res resid="#601">
   <dtitle>
    <xmbtext>Megjelent egy frissítés a Picasához</xmbtext>
   </dtitle>
   <dlayout>rect(0 0 275 90)</dlayout>
   <item itemid="1" itemtype="Button" layout="rect(7 43 134 59)">
    <xmbtext>Igen, töltse le most</xmbtext>
   </item>
   <item itemid="2" itemtype="Button" layout="rect(141 43 268 59)">
    <xmbtext>Nem, töltse le később</xmbtext>
   </item>
   <item itemid="1020" itemtype="Static" layout="rect(35 11 268 35)">
    <xmbtext>A Picasához elérhető egy ingyenes frissítés. \nLetölti most az új változatot?</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(6 10 26 30)">
    <xmbtext>id=106</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(7 69 268 85)">
    <xmbtext>Megjegyzés: a frissítés telepítéséhez rendszergazdai jogosultságokkal kell rendelkeznie.</xmbtext>
   </item>
  </res>
  <res resid="#602">
   <dtitle>
    <xmbtext>A Picasa frissítése kész</xmbtext>
   </dtitle>
   <dlayout>rect(0 0 257 84)</dlayout>
   <item itemid="1" itemtype="Button" layout="rect(14 50 127 66)">
    <xmbtext>Igen, frissítsen most</xmbtext>
   </item>
   <item itemid="2" itemtype="Button" layout="rect(131 50 244 66)">
    <xmbtext>Később, kérem</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(43 7 233 31)">
    <xmbtext>A Picasa készen áll egy frissítés telepítésére.\nÚjraindítja a Picasát, és telepíti most a frissítést?</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(14 7 34 27)">
    <xmbtext>id=106</xmbtext>
   </item>
  </res>
  <res resid="#603">
   <dtitle>
    <xmbtext>A Picasa frissítése kész</xmbtext>
   </dtitle>
   <dlayout>rect(0 0 257 84)</dlayout>
   <item itemid="1" itemtype="Button" layout="rect(14 50 127 66)">
    <xmbtext>Frissítés most</xmbtext>
   </item>
   <item itemid="2" itemtype="Button" layout="rect(131 50 244 66)">
    <xmbtext>Kérdezze meg később</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(43 7 233 31)">
    <xmbtext>A Picasa készen áll egy frissítés telepítésére.\nLehetséges, hogy a program felkéri, hogy lépjen be rendszergazdaként.</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(14 7 34 27)">
    <xmbtext>id=106</xmbtext>
   </item>
  </res>
 </RT_DIALOG>
</Win32Res>
