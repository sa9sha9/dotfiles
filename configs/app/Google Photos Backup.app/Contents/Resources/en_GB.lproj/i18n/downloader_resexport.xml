<?xml version="1.0" encoding="utf-8" ?>
<Win32Res>
 <RT_DIALOG>
  <res resid="#601">
   <dtitle>
    <xmbtext>A Picasa Update is Available</xmbtext>
   </dtitle>
   <dlayout>rect(0 0 275 90)</dlayout>
   <item itemid="1" itemtype="Button" layout="rect(7 43 134 59)">
    <xmbtext>Yes, Download Now</xmbtext>
   </item>
   <item itemid="2" itemtype="Button" layout="rect(141 43 268 59)">
    <xmbtext>No, Download Later</xmbtext>
   </item>
   <item itemid="1020" itemtype="Static" layout="rect(35 11 268 35)">
    <xmbtext>A free update to Picasa is available. \nDo you want to download the new version now?</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(6 10 26 30)">
    <xmbtext>id=106</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(7 69 268 85)">
    <xmbtext>Note: You must have administrative rights on this computer to install the update.</xmbtext>
   </item>
  </res>
  <res resid="#602">
   <dtitle>
    <xmbtext>Picasa update is ready</xmbtext>
   </dtitle>
   <dlayout>rect(0 0 257 84)</dlayout>
   <item itemid="1" itemtype="Button" layout="rect(14 50 127 66)">
    <xmbtext>Yes, update now</xmbtext>
   </item>
   <item itemid="2" itemtype="Button" layout="rect(131 50 244 66)">
    <xmbtext>Later, please</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(43 7 233 31)">
    <xmbtext>Picasa is ready to install an update.\nRestart Picasa and install this update now?</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(14 7 34 27)">
    <xmbtext>id=106</xmbtext>
   </item>
  </res>
  <res resid="#603">
   <dtitle>
    <xmbtext>Picasa update is ready</xmbtext>
   </dtitle>
   <dlayout>rect(0 0 257 84)</dlayout>
   <item itemid="1" itemtype="Button" layout="rect(14 50 127 66)">
    <xmbtext>Update now</xmbtext>
   </item>
   <item itemid="2" itemtype="Button" layout="rect(131 50 244 66)">
    <xmbtext>Ask later</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(43 7 233 31)">
    <xmbtext>Picasa is ready to install an update.\nYou may be prompted to log in as an administrator.</xmbtext>
   </item>
   <item itemid="-1" itemtype="Static" layout="rect(14 7 34 27)">
    <xmbtext>id=106</xmbtext>
   </item>
  </res>
 </RT_DIALOG>
